{
  "name": "rocket-science",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f .env.development nuxt",
    "dev:test": "env-cmd -f .env.production nuxt",
    "build": "env-cmd -f .env.production nuxt build",
    "build:dev": "env-cmd -f .env.development nuxt build",
    "start": "env-cmd -f .env.production nuxt start",
    "start:dev": "env-cmd -f .env.development nuxt start",
    "generate": "env-cmd -f .env.production nuxt generate",
    "generate:dev": "env-cmd -f .env.development nuxt generate",
    "lint:js": "eslint --ext \".ts,.js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint:fix-style": "stylelint **/*.{vue,css,scss} --fix",
    "lint:fix-code": "eslint --fix --ext .ts,.js,.vue --ignore-path .gitignore .",
    "lint:fix": "yarn lint:fix-code && yarn lint:fix-style",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{scss,css,vue}": "stylelint"
  },
  "dependencies": {
    "@nuxt/content": "^1.14.0",
    "@nuxt/http": "^0.6.4",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/composition-api": "^0.29.3",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/svg-sprite": "^0.5.2",
    "@nuxtjs/toast": "^3.3.1",
    "@types/deep-diff": "^1.0.1",
    "@walletconnect/web3-provider": "^1.6.6",
    "bignumber.js": "^9.0.1",
    "browser-detect": "^0.2.28",
    "core-js": "^3.15.1",
    "date-fns": "^2.25.0",
    "debounce": "^1.2.1",
    "deep-diff": "^1.0.2",
    "env-cmd": "^10.1.0",
    "ethers": "^5.4.7",
    "lint-staged": "^12.3.4",
    "normalize.css": "^8.0.1",
    "nuxt": "^2.15.7",
    "walletlink": "^2.1.11",
    "web3": "^1.6.0",
    "web3-eth-contract": "^1.6.1",
    "web3modal": "^1.9.4"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@nuxt/types": "^2.15.7",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/device": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@types/debounce": "^1.2.1",
    "babel-core": "7.0.0-bridge.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.19.1",
    "husky": "^7.0.2",
    "prettier": "^2.4.1",
    "sass": "^1.42.1",
    "sass-loader": "^10.1.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0"
  }
}
